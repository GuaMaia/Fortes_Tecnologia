
CREATE TABLE IF NOT EXISTS TBABASTECIMENTOS (
    ID                 INTEGER         PRIMARY KEY,
    BOMBA_ID           INTEGER,
    DATA_ABASTECIMENTO DATE            NOT NULL,
    QTD                NUMERIC (12, 3) NOT NULL,
    VALOR              NUMERIC (12, 3) NOT NULL,
    VALOR_IMPOSTO      NUMERIC (12, 3) NOT NULL,
    FOREIGN KEY (BOMBA_ID) REFERENCES TBBOMBAS (ID) 
);